{
  "all": true,
  "include": [
    "lib/**/*.js",
    "subway.js"
  ],
  "exclude": [
    "node_modules/**",
    "test/**",
    "tmp/**",
    "coverage/**",
    "bower_components/**",
    "plugin_cache/**",
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ],
  "report-dir": "./coverage",
  "temp-dir": "./coverage/.nyc_output",
  "check-coverage": false,
  "lines": 60,
  "statements": 60,
  "functions": 50,
  "branches": 50,
  "watermarks": {
    "lines": [60, 80],
    "functions": [50, 75],
    "branches": [50, 75],
    "statements": [60, 80]
  }
}
